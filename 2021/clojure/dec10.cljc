(require '[clojure.string :as str])

(def updaters {\( [\( inc]
               \) [\( dec]
               \< [\< inc]
               \> [\< dec]
               \[ [\[ inc]
               \] [\[ dec]
               \{ [\{ inc]
               \} [\{ dec]})

(def openers (set "([{<"))
(def closers {\( \)
              \[ \]
              \< \>
              \{ \}})

(defn do-stuff [line]
  (loop [opened '()
         [c & tail] line]
    (cond (openers c) (recur (conj opened c) tail)
          (= c (closers (first opened))) (recur (rest opened) tail)
          (nil? c) [opened []]
          :else [opened (cons c tail)])))

(def scores {\) 3
             \] 57
             \} 1197
             \> 25137})

(defn get-score [[opened [c & tail]]]
  (get scores c 0))

(get-score "[({(<(())[]>[[{[]{<()<>>")

(defn dec10 [input]
  (->> input
       (str/split-lines)
       (map do-stuff)
       (map get-score)
       (apply +)))

(dec10 input-full)

(def completion-scores {\( 1
                        \[ 2
                        \{ 3
                        \< 4})

(defn get-completion-score [[opened restline]]
  (reduce (fn [sum c]
            (+ (* sum 5)
               (completion-scores c)))
          0 opened))

(defn get-middle [l]
  (nth l (/ (dec (count l))
            2)))

(defn dec10-extra [input]
  (->> input
       (str/split-lines)
       (map do-stuff)
       (filter (comp zero? get-score))
       (map get-completion-score)
       (sort)
       (get-middle)))

(dec10-extra input-full)

